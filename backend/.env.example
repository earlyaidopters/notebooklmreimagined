# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Gemini API
GOOGLE_API_KEY=your_google_gemini_api_key

# OpenRouter API (Optional - enables multi-LLM support)
OPENROUTER_API_KEY=your_openrouter_api_key

# AtlasCloud Video API (Optional)
ATLASCLOUD_API_KEY=your_atlascloud_api_key

# LLM Provider Configuration
# Options: "google" or "openrouter"
DEFAULT_LLM_PROVIDER=google

# OpenRouter Configuration
OPENROUTER_DEFAULT_MODEL=anthropic/claude-3.5-sonnet
OPENROUTER_PROVIDER=  # Optional: specific OpenRouter provider (e.g., "Anthropic")

# App Configuration
APP_NAME=NotebookLM Reimagined
DEBUG=false
